    <table class = "table">
        <tr>
            <th>title</th>
            <th>date_of_admission</th>
            <th>image</th>
            <th>content</th>
        </tr>
        
        <% @posts.each do |post| %>
            <tr>
                <td><%=post.title%></td>
                <td><%=post.created_at%></td>
                <td><%=post.image_url%></td>
                <td><%=post.content%></td>
            </tr>
        <%end%>
   </table>
